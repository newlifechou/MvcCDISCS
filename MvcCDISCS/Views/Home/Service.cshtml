@model  IEnumerable<MvcCDISCS.Models.service>
@{
    ViewBag.PageTitle = "技术服务";
    Layout = "~/Views/Shared/_FrontEndContent.cshtml";
}

@section leftpart{
    <div class="partmenu">
        <h3 class="titlebar">联系我们</h3>
        <ul>
            <li><a href="~/Home/Contact">联系方式</a></li>
        </ul>
    </div>
}

@section rightpart{
    @if (Model.Count() > 0)
    {
        foreach (var item in Model)
        {
            <div class="serviceitem">
                <h5 class="serviceitemheader">@item.Title</h5>
                <img src="~/Images/service/@(item.Photo).jpg" alt="service" class="serviceitemphoto smallphotobg" />
                <div class="serviceitemcontent htmlcontent">
                    @Html.Raw(item.Content)
                </div>
            </div>
        }
    }
    else
    {
        <p>暂时没有服务项目，敬请稍后</p>
    }
}

@section scripts2{
    <script src="~/Scripts/js/service.js"></script>
}